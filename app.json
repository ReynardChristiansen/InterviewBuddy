{
  "expo": {
    "name": "InterviewBuddy",
    "slug": "InterviewBuddy",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gobigobi.InterviewBuddy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gobigobi.InterviewBuddy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPath":true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7d55e254-88da-49a1-b1bc-7e3f13f36a89"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "permissions":[
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE"
    ],
    "updates": {
      "url": "https://u.expo.dev/7d55e254-88da-49a1-b1bc-7e3f13f36a89"
    }
  }
}
